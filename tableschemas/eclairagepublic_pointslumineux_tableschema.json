			"title": "Type de source",
			"type": "string",
                        "constraints": {
                          "enum": ["LED", "SHP", "IM", "SBP", "INC", "HAL", "TF", "FC", "VM", "IND", "XEN", "AU", "XX"]
                         }
		},
		{
			"name": "ULR",
			"title": "ULR",
			"type": "integer"
		},
		{
			"name": "Température de couleur",
